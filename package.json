{
  "name": "parro",
  "version": "1.0.1",
  "description": "Desktop Parro App",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "package:darwin": "electron-packager . --icon=assets/icon/icon.icns --platform=darwin",
    "package:mas": "electron-packager . --icon=assets/icon/icon.icns --platform=mas",
    "package": "run-p \"package:* -- --overwrite\""
  },
  "repository": "https://github.com/scarhand/parro-electron",
  "keywords": [
    "Parro"
  ],
  "author": "Niels van der Zanden",
  "license": "MIT",
  "devDependencies": {
    "electron": "^18.3.7",
    "electron-packager": "^14.2.1"
  },
  "dependencies": {
    "yarn-run-all": "^3.1.1"
  }
}
